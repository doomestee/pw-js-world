{
  "name": "pw-js-world",
  "version": "0.4.1",
  "description": "An optional package for PW-JS-Api, aims to serve world purposes.",
  "scripts": {
    "test": "bun test/index.ts",
    "build:ts": "tsc -p tsconfig-cm.json & tsc -p tsconfig-esm.json",
    "build": "rimraf cm esm && npm run build:ts && ncp lib/types esm/types",
    "prepare": "npm run build"
  },
  "exports": {
    "types": "./esm/index.js",
    "import": "./esm/index.js",
    "require": "./cm/index.js"
  },
  "keywords": [
    "PixelWalker",
    "Util",
    "Utilities",
    "Misc"
  ],
  "author": "Doomester",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doomestee/pw-js-world.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@favware/npm-deprecate": "^2.0.0",
    "@types/node": "^22.10.5",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "pw-js-api": ">=0.3.0"
  }
}